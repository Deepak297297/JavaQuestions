import java.util.*;
public class Main{
    public static int Factorial(int n){
        if(n==1 || n==0){
            return 1;
        }
        int fact = Factorial(n-1);
        int result = n*fact;
        return result;
    }
	public static void main(String[] args) {
		int n = 5;
		int print = Factorial(n);
		System.out.println(print);
	}
}
